local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mixt_pes roball hub (BETA)", "Sentinel")
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")
PlayerSection:NewSlider("Walkspeed", "run faster", 500, 16, function(s) -- 500 (MaxValue) | 16 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
PlayerSection:NewButton("LongArm (press y to enable u to disable)", "Big reach", function(v)
	print("Clicked")
	loadstring(game:HttpGet('https://pastebin.com/raw/hSnaA1QX'))()
end)
PlayerSection:NewButton("Phenom gui", "phenom scripts", function(v)
	print("Clicked")
	loadstring(game:HttpGet('https://raw.githubusercontent.com/ashdasgdsfg/long-arm/main/source'))()
end)
PlayerSection:NewButton("loompic", "aimbot for lba not hoopz", function(v)
	print("Clicked")
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Loompic/Phenom-Aimbot/main/Bot"))()
end)
PlayerSection:NewButton("hoopz gui", "hoopz hack", function(v)
	print("Clicked")
	loadstring(game:HttpGet("https://paste.ee/r/GWYx2", true))()
end)
PlayerSection:NewButton("league ball mag (press f)", "ball mag for leagues", function(v)
	print("Clicked")
	loadstring(game:HttpGet("https://pastebin.com/raw/P0x1LmY1", true))()
end)
PlayerSection:NewButton("eau camlock (hold x)", "", function(v)
	print("Clicked")
	loadstring(game:HttpGet("https://raw.githubusercontent.com/ashdasgdsfg/rsddas/main/sadas", true))()
end)
